{
  "name": "Usuario",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellidos": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "miembros": {
      "type": "belongsTo",
      "model": "ListaFamiliar",
      "foreignKey": ""
    },
    "Solicitud": {
      "type": "hasAndBelongsToMany",
      "model": "ListaFamiliar",
      "foreignKey": ""
    },
    "Coordinador": {
      "type": "hasOne",
      "model": "ListaFamiliar",
      "foreignKey": "CoordinadorId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "aceptarSolicitud"
    }
  ],
  "methods": {
    "prototype.aceptarSolicitud": {
      "accepts": [
        {
          "arg": "context",
          "type": "object",
          "required": true,
          "description": "contexto de la aplicacion",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": [
        {
          "arg": "array",
          "type": "array",
          "root": true,
          "description": "array de vuelta"
        }
      ],
      "description": "acepta la solicitud",
      "http": [
        {
          "path": "/aceptarSolicitud",
          "verb": "put"
        }
      ]
    }
  }
}
